#include <bits/stdc++.h>

using namespace std;

int x = 100;

int main() {
  int n;
  cin >> n; //input
  
  // Print i chut n-i in range 1-n
  for (int i = 1; i <= n; i++) {
    cout << i << " chut " << n - i << " lund\n";
  }
  
  // Print the global variable x 
  cout << x << endl;

  return 0;// denotes successful completion
}
